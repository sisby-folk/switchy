{
	"schema_version": 1,
	"quilt_loader": {
		"group": "folk.sisby",
		"id": "switchy-compat-ui",
		"version": "${switchy}",
		"metadata": {
			"name": "Switchy Compat UI",
			"description": "UI compatibility for built-in switchy modules like styled nicknames, fabric tailor, and origins.",
			"contributors": {
				"Sisby folk": "Co-Authors"
			},
			"contact": {
				"homepage": "https://modrinth.com/mod/switchy",
				"issues": "https://github.com/sisby-folk/switchy/issues",
				"sources": "https://github.com/sisby-folk/switchy"
			},
			"icon": "assets/switchy/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"switchy_client": [
				"folk.sisby.switchy.client.SwitchyCompatClient"
			]
		},
		"depends": [
			{
				"id": "minecraft",
				"versions": ">=${mc}"
			},
			{
				"id": "fabricloader",
				"versions": ">=${fl}"
			},
			{
				"id": "fabric-api",
				"versions": ">=${fapi}"
			},
			{
				"id": "switchy-core",
				"versions": "${switchy}"
			},
			{
				"id": "switchy-client",
				"versions": "${switchy}"
			},
			{
				"id": "switchy-ui",
				"versions": "${switchy}"
			},
			{
				"id": "styled-nicknames",
				"versions": ">=${styledNicknames}",
				"optional": true
			},
			{
				"id": "fabrictailor",
				"versions": ">=${fabrictailor}",
				"optional": true
			},
			{
				"id": "origins",
				"versions": ">=${origins}",
				"optional": true
			}
		]
	},
	"modmenu": {
		"parent": "switchy"
	},
	"minecraft": {
		"environment": "client"
	}
}
