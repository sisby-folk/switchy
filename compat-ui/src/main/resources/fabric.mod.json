  "depends": {
    "minecraft": ">=${mc}",
    "fabricloader": ">=0.14.9",
    "fabric": ">=${fapi}",
    "switchy-core": ">=${switchy}",
    "switchy-client": ">=${switchy}",
    "switchy-ui": ">=${switchy}"
